{
    "extends": ["next/core-web-vitals"],
    "parserOptions": {
        "project": ["./tsconfig.json"]
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "indent": ["error", 4],
        "no-multi-spaces": ["error"],
        "no-multiple-empty-lines": ["error"]
    }
}
